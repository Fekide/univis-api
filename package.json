    "esbuild": "^0.17.18",
    "eslint": "^8.39.0",
    "jest": "29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.6",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "fast-xml-parser": "^4.2.2"
  }
